<h3 class="form-heading">ログイン</h3>
<div class="container">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-group">
    <%= f.label :email, "メールアドレス" , class: "devise-form"%><br />
    <%= f.email_field :email, class: "form-control", autofocus: true, autocomplete: "email" %>
  </div>

  <div class="form-group">
    <%= f.label :password, "パスワード", class: "devise-form" %><br />
    <%= f.password_field :password, class: "form-control",autocomplete: "current-password" %>
  </div>

  <div class="actions">
    <%= f.submit "ログインする", class: "btn btn-success" %>
  </div>
</div>

<% end %>

<%= render "devise/shared/links" %>

